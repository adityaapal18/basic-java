class program2
{
    public static void main(String args[])
    {
        int num1=5;
        int num2=10;
        int a=num1+num2;
        int b=num1-num2;
        int c=num1*num2;
        int d=num1/num2;
        int e=num1%num2;
        System.out.println(a);
        System.out.println(b);
        System.out.println(c);
        System.out.println(d);
        System.out.println(e);
    }
}
